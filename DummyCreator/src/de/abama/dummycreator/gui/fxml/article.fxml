   <children>
         <ImageView fx:id="image" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
			 <image>
				<Image url="@../image/icon_artikel.png" />
			 </image>
         <HBox.margin>
            <Insets right="5.0" />
         </HBox.margin>
      </ImageView>
      <VBox prefHeight="250.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
         <children>
            <Label fx:id="number" text="Artikelnummer" />
            <Label fx:id="title" layoutX="10.0" layoutY="10.0" text="Titel">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <Label fx:id="description" layoutX="10.0" layoutY="27.0" text="Beschreibung" textOverrun="CLIP" />
         </children>
      </VBox>
   </children>
</HBox>
